<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
     xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container"
            granite:class="cmp-image__editor">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <slide1
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Slide 1"
                            sling:resourceType="acs-commons/granite/ui/components/include"
                            path="/apps/adaptdialog/components/hero/tab_slide"
                            namespace="slide1"
                            margin="{Boolean}true">
                            <parameters
                                    jcr:primaryType="nt:unstructured"
                                    extraCssClassCtaLeft="slide1-left"
                                    extraCssClassCtaRight="slide1-right"
                            />
                    </slide1>
                    <slide2
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Slide 2"
                            sling:resourceType="acs-commons/granite/ui/components/include"
                            path="/apps/adaptdialog/components/hero/tab_slide"
                            namespace="slide2"
                            margin="{Boolean}true">
                        <parameters
                                jcr:primaryType="nt:unstructured"
                                extraCssClassCtaLeft="slide2-left"
                                extraCssClassCtaRight="slide2-right"
                        />
                    </slide2>
                    <slide3
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Slide 3"
                            sling:resourceType="acs-commons/granite/ui/components/include"
                            path="/apps/adaptdialog/components/hero/tab_slide"
                            namespace="slide3"
                            margin="{Boolean}true">
                        <parameters
                                jcr:primaryType="nt:unstructured"
                                extraCssClassCtaLeft="slide3-left"
                                extraCssClassCtaRight="slide3-right"/>
                    </slide3>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
